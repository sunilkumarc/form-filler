{
  "manifest_version": 2,

  "name": "Form Filler",
  "description": "Form filler extension helps you to fill input fields on any web page.",
  "version": "0.0.1",

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentscript.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "48": "images/icon_48_3.png"
  }
}
